function getWordsBeginWith(words, begin_char)  {
var return_words = [];
var small_char.toLower Case();
for (var i = 0; i < words.length; i++) {
if (words[i].startsWith('begin_char') || words[i].startsWith('begin_char'))
return_words.push(words[i]);
}
return return_words;
}
